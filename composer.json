{
    "name": "lob/lob-php",
    "type": "library",
    "description": "Lob.com API PHP wrapper.",
    "keywords": ["lob", "printing", "api", "rest", "web service", "wrapper"],
    "homepage": "https://www.lob.com/",
    "license": "MIT",
    "authors": [{
        "name": "Lob",
        "email": "support@lob.com",
        "homepage": "https://www.lob.com"
    }],
    "require": {
        "php": ">=7.4.0",
        "guzzlehttp/guzzle": "^7.3"
    },
    "autoload": {
        "psr-0": {
            "Lob\\Tests": "tests/",
            "Lob": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "satooshi/php-coveralls": "2.*||1.*",
        "dompdf/dompdf": "0.6.* || 1.2.*"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
